        {
            "name": "HBox Form",
            "iconCls": "x-fa fa-code",
            "view": {
                "xtype": "panel",
                "title": "Address Card",
                "bodyPadding": 20,
                "width": 500,
                "autoSize": true,
                "layout": "hbox",
                "items": [
                    {
                        "xtype": "container",
                        "flex": 1,
                        "width": "100%",
                        "margin": "0 5 0 0",
                        "autoSize": true,
                        "items": [
                            {
                                "xtype": "textfield",
                                "width": "100%",
                                "flex": 1,
                                "bodyAlign": "stretch",
                                "label": "First Name"
                            },
                            {
                                "xtype": "textfield",
                                "flex": 1,
                                "bodyAlign": "stretch",
                                "width": "100%",
                                "label": "Company"
                            }
                        ]
                    },
                    {
                        "xtype": "container",
                        "flex": 1,
                        "width": "100%",
                        "margin": "0 0 0 5",
                        "autoSize": true,
                        "items": [
                            {
                                "xtype": "textfield",
                                "width": "100%",
                                "flex": 1,
                                "bodyAlign": "stretch",
                                "label": "Last Name"
                            },
                            {
                                "xtype": "emailfield",
                                "flex": 1,
                                "bodyAlign": "stretch",
                                "width": "100%",
                                "label": "Email",
                                "validators": "email"
                            }
                        ]
                    }
                ],
                "buttons": [
                    "->",
                    {
                        "text": "Save"
                    },
                    {
                        "text": "Cancel"
                    }
